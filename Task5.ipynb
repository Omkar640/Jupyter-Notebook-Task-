    "A = int(input(\"Enter number A: \"))\n",
    "B = int(input(\"Enter number B: \"))\n",
    "\n",
    "count = 0\n",
    "\n",
    "while A % B == 0:\n",
    "    A = A // B\n",
    "    count += 1\n",
    "\n",
    "if count > 0:\n",
    "    print(\"Number is purely divisible\")\n",
    "    print(\"It is divisible\", count, \"times\")\n",
    "else:\n",
    "    print(\"Number is not purely divisible\")\n"
